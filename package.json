{
	"name": "@prettier/plugin-pug",
	"version": "1.1.3",
	"description": "Prettier Pug Plugin",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write 'src/**/*.ts'",
		"lint": "yarn lint:src && yarn lint:test",
		"lint:src": "eslint --ext .ts src",
		"lint:test": "eslint --ext .test.ts test",
		"test": "jest"
	},
	"keywords": [
		"pug",
		"prettier",
		"prettier-plugin"
	],
	"author": {
		"name": "Christopher Quadflieg",
		"email": "chrissi92@hotmail.de",
		"url": "https://github.com/Shinigami92"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/prettier/plugin-pug.git"
	},
	"bugs": "https://github.com/prettier/plugin-pug/issues",
	"license": "MIT",
	"dependencies": {
		"pug-lexer": "~4.1.0"
	},
	"devDependencies": {
		"@types/jest": "~24.0.25",
		"@types/node": "~13.1.4",
		"@types/prettier": "~1.19.0",
		"@typescript-eslint/eslint-plugin": "~2.14.0",
		"@typescript-eslint/parser": "~2.14.0",
		"eslint": "~6.8.0",
		"eslint-config-prettier": "~6.9.0",
		"eslint-plugin-prettier": "~3.1.2",
		"jest": "~24.9.0",
		"jest-junit": "~10.0.0",
		"prettier": "1.19.1",
		"ts-jest": "~24.2.0",
		"typescript": "~3.7.4"
	},
	"peerDependencies": {
		"prettier": "^1.18.0"
	}
}
